{
  "name": "santa-app",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "main": "server.js",
  "scripts": {
    "start": "npm run test:coverage && node index.js",
    "start:server": "node -r dotenv/config server/index.js",
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0",
    "nodemailer": "^6.9.14"
  },
  "devDependencies": {
    "@types/node": "^22.5.1",
    "@types/react": "^18.0.34",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "concurrently": "^6.2.0",
    "nodemon": "^3.1.4",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=16.x"
  },
  "repository": {
    "url": "https://github.com/alj-devops/santa-app"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ]
}